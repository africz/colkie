{
  "name": "Colkie",
  "version": "0.0.1",
  "description": "Colkie Code Challange",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run eslint && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild && clear",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "killall node && npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "premigrate": "npm run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "npm run rebuild",
    "start": "node -r source-map-support/register .",
    "debug": "node --inspect-brk=9229 -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "npm run clean && npm run build",
    "testserver": "cd ./src/test/testserver && npm run start&"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Attila Fricz <attila.fricz@gmail.com>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^5.0.10",
    "@loopback/core": "^4.0.10",
    "@loopback/repository": "^5.1.5",
    "@loopback/rest": "^12.0.10",
    "@loopback/rest-explorer": "^5.0.10",
    "@loopback/service-proxy": "^5.0.10",
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/nodemailer": "^6.4.7",
    "@types/nodemailer-mailgun-transport": "^1.4.3",
    "axios": "^0.26.1",
    "bcrypt": "^5.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "device-detector-js": "^3.0.2",
    "helmet": "^5.0.2",
    "http-errors": "^2.0.0",
    "json-circular-stringify": "^1.0.1",
    "jsonwebtoken": "^9.0.0",
    "loopback-boot": "^3.3.1",
    "loopback-connector-mysql": "^6.2.0",
    "loopback-connector-mysql2": "^6.0.0",
    "md5": "^2.3.0",
    "mysql2": "^2.3.3",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.9.1",
    "nodemailer-mailgun-transport": "^2.1.3",
    "npm": "^8.19.4",
    "randomstring": "^1.2.3",
    "register": "^0.0.0",
    "sdoc": "^0.0.3",
    "serve-favicon": "^2.5.0",
    "simple-node-logger": "^21.8.12",
    "tslib": "^2.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@loopback/build": "^9.0.10",
    "@loopback/testlab": "^5.0.10",
    "@types/bcryptjs": "^2.4.2",
    "@types/chai": "^4.3.4",
    "@types/node": "^14.18.42",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "chai": "^4.3.7",
    "clean-jsdoc-theme": "^3.3.4",
    "eslint": "^8.38.0",
    "jsdoc": "^3.6.10",
    "jsdoc-plugin-typescript": "^2.2.1",
    "mocha": "^10.2.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.1.3",
    "typedoc": "^0.23.28",
    "typedoc-plugin-mermaid": "^1.10.0",
    "typedoc-source-example": "file:/./typedoc-source-example/typedoc-source-example-0.0.2.tgz",
    "typescript": "~4.9.5"
  }
}
